         version="4.0">
    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>servelet.Servlet1</servlet-class>
        <init-param>
            <param-name>keya</param-name>
            <param-value>valuea1</param-value>
        </init-param>
        <init-param>
            <param-name>keyb</param-name>
            <param-value>valueb1</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/servlet1</url-pattern>
    </servlet-mapping>

    <context-param>
        <param-name>encoding</param-name>
        <param-value>UTF-8</param-value>
    </context-param>

    <context-param>
        <param-name>username</param-name>
        <param-value>zhangsan</param-value>
    </context-param>


